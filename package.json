{
  "name": "reverseats",
  "version": "1.0.0",
  "description": "A Chrome extension that counts and displays the number of table rows on WaterlooWorks job page",
  "main": "dist/content.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "tsc": "tsc",
    "copy-assets": "cp src/manifest.json dist/ && cp src/*.png dist/",
    "build-extension": "npm run build && npm run copy-assets"
  },
  "keywords": [
    "chrome-extension",
    "waterlooworks",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/jquery": "^3.5.32",
    "ts-loader": "^9.5.2",
    "typescript": "^5.3.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "canvas": "^3.1.0",
    "compromise": "^14.14.4",
    "jquery": "^3.7.1",
    "node-nlp": "^5.0.0-alpha.5"
  }
}
